    "import time\n",
    "import json\n",
    "import pandas as pd\n",
    "from sqlalchemy import text\n",
    "\n",
    "def validate_sql(engine, sql):\n",
    "    try:\n",
    "        with engine.connect() as connection:\n",
    "            result = connection.execute(text(sql))\n",
    "            df = pd.DataFrame(result.fetchall(), columns=result.keys())\n",
    "        return True, df\n",
    "    except Exception as e:\n",
    "        return False, str(e)\n",
    "\n",
    "def validate_result(engine, result, sql):\n",
    "    sql_valid, sql_result = validate_sql(engine, sql)\n",
    "    if not sql_valid:\n",
    "        return False, f\"SQL execution failed: {sql_result}\"\n",
    "    \n",
    "    if result.shape != sql_result.shape:\n",
    "        return False, f\"Shape mismatch. Expected: {sql_result.shape}, Got: {result.shape}\"\n",
    "    \n",
    "    if not result.equals(sql_result):\n",
    "        return False, \"Data mismatch between result and SQL execution\"\n",
    "    \n",
    "    return True, \"Result validated successfully\"\n",
    "\n",
    "def run_tests(query_engine, test_inputs, engine):\n",
    "    results = []\n",
    "    total_token_usage = {\n",
    "        \"embedding_tokens\": 0,\n",
    "        \"llm_prompt_tokens\": 0,\n",
    "        \"llm_completion_tokens\": 0,\n",
    "        \"total_llm_tokens\": 0\n",
    "    }\n",
    "\n",
    "    for i, query_group in enumerate(test_inputs):\n",
    "        print(f\"Running test group {i+1}/{len(test_inputs)}\")\n",
    "        for query in query_group:\n",
    "            print(f\"  Testing query: {query}\")\n",
    "            start_time = time.time()\n",
    "            try:\n",
    "                result, part_numbers, cot_response, token_usage = query_engine.process_user_query(query)\n",
    "                execution_time = time.time() - start_time\n",
    "\n",
    "                # Extract SQL from cot_response\n",
    "                sql_match = re.search(r'--- SQL ---\\s*(.*?)\\s*--- END SQL ---', cot_response, re.DOTALL)\n",
    "                sql = sql_match.group(1).strip() if sql_match else None\n",
    "\n",
    "                # Validate result\n",
    "                if sql:\n",
    "                    is_valid, validation_message = validate_result(engine, result, sql)\n",
    "                else:\n",
    "                    is_valid, validation_message = False, \"No SQL found in cot_response\"\n",
    "\n",
    "                test_result = {\n",
    "                    \"query\": query,\n",
    "                    \"success\": is_valid,\n",
    "                    \"execution_time\": execution_time,\n",
    "                    \"result_shape\": result.shape if result is not None else None,\n",
    "                    \"part_numbers_count\": len(part_numbers),\n",
    "                    \"token_usage\": token_usage,\n",
    "                    \"sql\": sql,\n",
    "                    \"validation_message\": validation_message,\n",
    "                    \"error\": None\n",
    "                }\n",
    "\n",
    "                # Update total token usage\n",
    "                for key in total_token_usage:\n",
    "                    total_token_usage[key] += token_usage[key]\n",
    "\n",
    "            except Exception as e:\n",
    "                execution_time = time.time() - start_time\n",
    "                test_result = {\n",
    "                    \"query\": query,\n",
    "                    \"success\": False,\n",
    "                    \"execution_time\": execution_time,\n",
    "                    \"error\": str(e)\n",
    "                }\n",
    "            \n",
    "            results.append(test_result)\n",
    "            print(f\"    {'Success' if test_result['success'] else 'Failed'} - Execution time: {execution_time:.2f}s\")\n",
    "            if not test_result['success']:\n",
    "                print(f\"    Error or Validation Message: {test_result.get('validation_message') or test_result.get('error')}\")\n",
    "            \n",
    "    return results, total_token_usage\n",
    "\n",
    "# Usage\n",
    "engine = create_engine(f'sqlite:///{db_path}')\n",
    "query_engine = IntegratedMultiStepQueryEngine(engine)\n",
    "test_results, total_token_usage = run_tests(query_engine, test_inputs, engine)\n",
    "\n",
    "# Calculate cost (assuming GPT-4 pricing)\n",
    "cost_per_1k_tokens = 0.00250 \n",
    "total_cost = (total_token_usage['llm_prompt_tokens'] / 1000) * cost_per_1k_tokens\n",
    "\n",
    "# Save results to a file\n",
    "output = {\n",
    "    \"test_results\": test_results,\n",
    "    \"total_token_usage\": total_token_usage,\n",
    "    \"total_cost\": total_cost\n",
    "}\n",
    "\n",
    "with open('/Users/skylerwilson/Desktop/PartsWise/co-pilot-v1/notebooks/test_results.json', 'w') as f:\n",
    "    json.dump(output, f, indent=2)\n",
    "\n",
    "print(f\"Test results saved to /Users/skylerwilson/Desktop/PartsWise/co-pilot-v1/notebooks/test_results.json\")\n",
    "print(f\"Total token usage: {total_token_usage}\")\n",
    "print(f\"Estimated total cost: ${total_cost:.2f}\")"
