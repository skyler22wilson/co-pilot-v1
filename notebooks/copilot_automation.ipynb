    "                    if 'part_number' in result.columns:\n",
    "                        part_numbers = set(result['part_number'])\n",
    "                else:\n",
    "                    # Store intermediate result in a temporary table\n",
    "                    temp_table_name = f\"temp_result_{i}\"\n",
