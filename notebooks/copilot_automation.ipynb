    "from sqlalchemy import create_engine, text\n",
    "from llama_index.core import SQLDatabase, VectorStoreIndex\n",
    "from llama_index.core.objects import SQLTableNodeMapping, ObjectIndex, SQLTableSchema\n",
    "from llama_index.core.indices.struct_store import SQLTableRetrieverQueryEngine\n",
    "from llama_index.llms.openai import OpenAI\n",
    "from llama_index.core.callbacks import CallbackManager, TokenCountingHandler\n",
    "import pandas as pd\n",
    "import re\n",
    "import tiktoken\n",
    "\n",
    "class IntegratedMultiStepQueryEngine:\n",
    "    def __init__(self, engine):\n",
    "        self.engine = engine\n",
    "        self.token_counter = TokenCountingHandler(\n",
    "            tokenizer=tiktoken.encoding_for_model(\"gpt-4o-mini-2024-07-18\").encode\n",
    "        )\n",
    "        self.callback_manager = CallbackManager([self.token_counter])\n",
    "        self.query_engine, self.context_str = self.setup_nlsql_query_engine()\n",
    "        self.temp_tables = {}\n",
    "\n",
    "    def setup_nlsql_query_engine(self):\n",
    "        def initialize_table_objects():\n",
    "            sql_database = SQLDatabase(self.engine, sample_rows_in_table_info=2, include_tables=['parts', 'makes', 'models', 'part_years', 'part_make', 'part_model'])\n",
    "            \n",
    "            table_contexts = {\n",
    "                'parts': \"\"\"The 'parts' table is the primary table for storing individual part information.\n",
    "                            Includes columns 'id' (PK), 'part_number' (unique), 'name', 'quantity', 'price', 'months_no_sale'.\n",
    "                            'months_no_sale' refers to the number of months the part has not been sold.\n",
    "                            Has many-to-many relationships with 'makes' and 'models' tables, and a one-to-many relationship with 'part_years'.\"\"\",\n",
    "                'makes': \"\"\"The 'makes' table stores car manufacturers or brands.\n",
    "                            Includes columns 'id' (PK) and 'name' (unique).\n",
    "                            Has a many-to-many relationship with 'parts' table through 'part_make' table.\n",
    "                            Use when querying about specific car brands or manufacturers.\"\"\",\n",
    "                'models': \"\"\"The 'models' table stores car models.\n",
    "                             Includes columns 'id' (PK) and 'name' (unique).\n",
    "                             Has a many-to-many relationship with 'parts' table through 'part_model' table.\n",
    "                             Use when querying about specific car models.\"\"\",\n",
    "                'part_years': \"\"\"The 'part_years' table stores the years each part is compatible with.\n",
    "                                 Includes columns 'id' (PK), 'part_id' (FK to parts.id), and 'year'.\n",
